{
  "name": "singlefileapproach",
  "version": "1.0.0",
  "description": "Single import treeshakable approach",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "generate": "gulp --require ts-node/register/transpile-only",
    "script:filled": "node -r ts-node/register scripts/index.ts --target=filled",
    "script:animated": "node -r ts-node/register scripts/index.ts --target=animated",
    "script:outlined": " node -r ts-node/register scripts/index.ts --target=outlined",
    "build:es": "tsc --project tsconfig.build.json --module esnext --outDir es",
    "build:lib": "tsc --project tsconfig.build.json --module commonjs --outDir lib",
    "build": "npm run build:es && npm run build:lib",
    "publish": "npm run generate && mkdir -p src/icon && npm run script:filled && npm run script:outlined && rollup -c"
  },
  "author": "Vinayak",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@types/graceful-fs": "^4.1.5",
    "@types/gulp": "^4.0.8",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "babel-loader": "^8.2.2",
    "del": "^6.0.0",
    "eos-icons": "^5.1.0",
    "eslint": "^7.28.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "lodash": "^4.17.21",
    "rollup": "^2.51.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "files": [
    "src",
    "dist"
  ],
  "release": {
    "branches": [
      "master"
    ]
  }
}
